{
  "name": "data-template",
  "version": "0.0.1",
  "description": "A template for assets data-packages",
  "keywords": [
    "data",
    "template"
  ],
  "license": "MIT",
  "author": {
    "name": "name",
    "email": "email"
  },
  "repository": {
    "type": "git",
    "url": "git-url"
  },
  "private": true,
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "changelog42": "0.9.0",
    "eslint": "3.4.0",
    "mgl-eslint-rules-browser": "1.3.0"
  },
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run lint",
    "log": "changelog42 --no-author --commit-url=https://github.com/magora-labs/data-template/commit",
    "clean": "rm -rf node_modules/ && rm -f npm-debug.log",
    "postinstall": "bower i && if test -d .git/hooks; then echo '#!/bin/sh\\nnpm test' > .git/hooks/pre-commit && chmod u+x .git/hooks/pre-commit; fi"
  }
}
