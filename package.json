{
  "name": "data-template",
  "version": "0.0.1",
  "description": "A template for assets data-packages",
  "keywords": [
    "data",
    "template"
  ],
  "license": "MIT",
  "author": {
    "name": "name",
    "email": "email"
  },
  "repository": {
    "type": "git",
    "url": "git-url"
  },
  "private": true,
  "dependencies": {},
  "optionalDependencies": {},
  "devDependencies": {
    "changelog42": "0.7.0",
    "eslint": "1.9.0",
    "htmlhint": "0.9.12",
    "istanbul": "0.4.0",
    "mocha": "2.3.4",
    "next-update": "0.9.5",
    "sass-lint": "1.3.3"
  },
  "scripts": {
    "eslint": "eslint ./",
    "sass-lint": "sass-lint -qv",
    "htmlhint": "htmlhint --config .htmlhintrc www/**/*.{htm,html,mustache}",
    "lint": "npm run eslint && npm run sass-lint && npm run htmlhint",
    "test": "npm run lint && istanbul cover _mocha -- ./test",
    "update": "next-update -k",
    "log": "changelog42 --no-author --commit-url=https://github.com/magora-labs/data-template/commit",
    "clean": "rm -rf node_modules/ && rm -rf coverage/ && rm -f npm-debug.log",
    "postinstall": "if test -d .git/hooks; then echo '#!/bin/sh\\nnpm test' > .git/hooks/pre-commit && chmod u+x .git/hooks/pre-commit; fi"
  }
}
